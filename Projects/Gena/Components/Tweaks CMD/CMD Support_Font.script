[Variables]
%ConsoleTestKey%=Testing_Cmd_Settings

[Process]
If,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Echo,"Components PluginCache"
If,Not,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Run,%ScriptFile%,Process_Normal

[Process_Normal]
If,%Lucida_lucon_ttf_CheckBox%,Equal,True,Run,%scriptfile%,lucon_add
If,%Default_Set_CheckBox%,Equal,True,Run,%scriptfile%,default_reg

[lucon_add]
Run,%ScriptFile%,lucon_ttf_add
Run,%ScriptFile%,lucon_reg

[lucon_ttf_add]
RegHiveLoad,Tmp_Software,%RegSoftware%
RegWrite,HKLM,0x1,"Tmp_Software\Microsoft\Windows NT\CurrentVersion\Fonts","Lucida Console (TrueType)",lucon.ttf
RegHiveUnLoad,Tmp_Software
Require,Font,lucon.ttf

[lucon_reg]
Set,%FontSizeValue%,%Lucida_FontSize_ScrollBox%
StrFormat,MULT,%FontSizeValue%,65536
RegHiveLoad,Tmp_Default,%RegDefault%
If,%Lucida_CheckBox%,Equal,True,Begin
//http://blogs.msdn.com/michkap/archive/2005/06/29/433669.aspx
//KEY== HKEY_CURRENT_USER\Console, ValueName == FaceName, Value == <whatever font you like>
//KEY== HKEY_CURRENT_USER\Console, ValueName == FontFamily, Value == <50 for decorative, 40 for Script, 30 for Modern, 20 for Swiss, or 10 for Roman>.
RegWrite,HKLM,0x1,Tmp_Default\Console,FaceName,"Lucida Console"
RegWrite,HKLM,0x4,Tmp_Default\Console,FontFamily,54
RegWrite,HKLM,0x4,Tmp_Default\Console,FontSize,%FontSizeValue%
If,%Lucida_BoldFonts_CheckBox%,Equal,True,RegWrite,HKLM,0x4,Tmp_Default\Console,FontWeight,700
Else,RegWrite,HKLM,0x4,Tmp_Default\Console,FontWeight,400
End
If,%Lucida_system32_CheckBox%,Equal,True,Begin
RegWrite,HKLM,0x1,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FaceName,"Lucida Console"
RegWrite,HKLM,0x4,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FontFamily,54
RegWrite,HKLM,0x4,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FontSize,%FontSizeValue%
If,%Lucida_BoldFonts_CheckBox%,Equal,True,RegWrite,HKLM,0x4,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FontWeight,700
Else,RegWrite,HKLM,0x4,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FontWeight,400
End
RegHiveUnLoad,Tmp_Default
If,%Lucida_HKLM_CheckBox%,Equal,True,Begin
RegHiveLoad,Tmp_Software,%RegSoftware%
RegWrite,HKLM,0x1,"Tmp_Software\Microsoft\Windows NT\CurrentVersion\Console\TrueTypeFont",0,Lucida#$sConsole
RegHiveUnLoad,Tmp_Software
End
If,%NtfsAllowExtChaIn8dot3Name_Check%,Equal,True,Begin
RegHiveLoad,Tmp_System,%RegSystem%
RegWrite,HKLM,0x4,Tmp_System\ControlSet001\Control\FileSystem,NtfsAllowExtendedCharacterIn8dot3Name,1
RegHiveUnLoad,Tmp_System
End

[default_reg]
RegHiveLoad,Tmp_Default,%RegDefault%
If,%Default_CheckBox%,Equal,True,Begin
//Default setting is No value (Null),
//Terminal means having 'Raster Fonts' but I do not think most of the world uses 'Raster Fonts' (not checked)
//RegWrite,HKLM,0x1,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FaceName,Terminal
RegWrite,HKLM,0x1,Tmp_Default\Console,FaceName,
RegWrite,HKLM,0x4,Tmp_Default\Console,FontFamily,48
//better to delete FontSize to set back default settings , reason is default is 'Raster Fonts' host PE have,
//value can be  4x6 6x8 8x8 16x8 5x12 7x12 8x12 16x12 12x16 10x18 --> due to 'Raster Fonts' PE contains !!!!!
//If required (I do not think so) one can make a Plugin (or add current Plugin) required fonts due to 'Raster Font' size...
If,ExistRegKey,HKLM,Tmp_Default\Console,FontSize,RegDelete,HKLM,Tmp_Default\Console,FontSize
RegWrite,HKLM,0x4,Tmp_Default\Console,FontWeight,400
End
If,%Default_System32_CheckBox%,Equal,True,Begin
//Default setting is No value (Null),
//Terminal means having 'Raster Fonts' but I do not think most of the world uses 'Raster Fonts' (not checked)
//RegWrite,HKLM,0x1,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FaceName,Terminal
RegWrite,HKLM,0x1,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FaceName,
RegWrite,HKLM,0x4,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FontFamily,48
//better to delete FontSize to set back default settings , reason is default is 'Raster Fonts' host PE have,
//value can be  4x6 6x8 8x8 16x8 5x12 7x12 8x12 16x12 12x16 10x18 --> due to 'Raster Fonts' PE contains !!!!!
//If required (I do not think so) one can make a Plugin (or add current Plugin) required fonts due to 'Raster Font' size...
If,ExistRegKey,HKLM,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FontSize,RegDelete,HKLM,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FontSize
RegWrite,HKLM,0x4,Tmp_Default\Console\#$pSystemRoot#$p_system32_cmd.exe,FontWeight,400
End
RegHiveUnLoad,Tmp_Default

[Process_Test]
// For tests, naturally %Lucida_lucon_ttf_CheckBox% is the (only) main switch as shown in interface, since it is hostos test nothing done to lucon.ttf
If,%Lucida_lucon_ttf_CheckBox%%Lucida_default_CheckBox%,Equal,TrueTrue,Begin
Set,%FontSizeValue%,%Lucida_FontSize_ScrollBox%
StrFormat,MULT,%FontSizeValue%,65536
RegWrite,HKCU,0x1,Console\%ConsoleTestKey%,FaceName,"Lucida Console"
RegWrite,HKCU,0x4,Console\%ConsoleTestKey%,FontFamily,54
RegWrite,HKCU,0x4,Console\%ConsoleTestKey%,FontSize,%FontSizeValue%
If,%Lucida_BoldFonts_CheckBox%,Equal,True,RegWrite,HKCU,0x4,Console,FontWeight,700
Else,RegWrite,HKCU,0x4,Console,FontWeight,400
End
If,%Default_Set_CheckBox%,Equal,True,Begin
RegWrite,HKCU,0x1,Console\%ConsoleTestKey%,FaceName,
RegWrite,HKCU,0x4,Console\%ConsoleTestKey%,FontFamily,48
If,ExistRegKey,HKCU,Console\%ConsoleTestKey%,FontSize,RegDelete,HKCU,Console\%ConsoleTestKey%,FontSize
RegWrite,HKCU,0x4,Console,FontWeight,400
End
ShellExecuteEx,Hide,cmd.exe,"/c Start #$q%ConsoleTestKey%#$q cmd.exe",%scriptdir%

[Process_Test_Delete_Reg]
If,ExistRegSection,HKCU,Console\%ConsoleTestKey%,RegDelete,HKCU,Console\%ConsoleTestKey%

