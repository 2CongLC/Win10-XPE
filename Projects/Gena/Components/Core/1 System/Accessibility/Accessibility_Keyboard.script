[Variables]

[Process]
If,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Echo,"Components PluginCache"
If,Not,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Run,%ScriptFile%,Process_Normal

[Process_Normal]
IniRead,%ProjectDir%\Script.Project,IniVariables,NoWarnRegOverWrite,%NoWarnROW%
Set,%StickyKeysFlagsValue%,26
If,%pCheckBox_Accessbility_Keyboard_StickyKeys%,Equal,True,StrFormat,inc,%StickyKeysFlagsValue%,1
If,%pCheckBox_KeyboardShortcut%,Equal,True,StrFormat,inc,%StickyKeysFlagsValue%,4
If,%pCheckBox_Options_twicetolock%,Equal,True,StrFormat,inc,%StickyKeysFlagsValue%,128
If,%pCheckBox_Options_twiceoff%,Equal,True,StrFormat,inc,%StickyKeysFlagsValue%,256
If,%pCheckBox_Notification_Sound%,Equal,True,StrFormat,inc,%StickyKeysFlagsValue%,64
If,%pCheckBox_Notification_Showstatus%,Equal,True,StrFormat,inc,%StickyKeysFlagsValue%,32
//
Set,%ToggleKeysFlagsValue%,58
If,%ToggleKeys_pCheckBox_UseToggleKeys%,Equal,True,StrFormat,inc,%ToggleKeysFlagsValue%,1
If,%ToggleKeys_pCheckBox_UseShortcut%,Equal,True,StrFormat,inc,%ToggleKeysFlagsValue%,4
//
If,%ShowExtraKeyboardHelp_pCheckBox%,Equal,True,Set,%KeyboardpreferenceValue%,1
If,%ShowExtraKeyboardHelp_pCheckBox%,Equal,False,Set,%KeyboardpreferenceValue%,0
//--
RegHiveLoad,Tmp_Default,%RegDefault%
If,%NoWarnROW%,Equal,True,System,ERROROFF,3
RegWrite,HKLM,0x1,"Tmp_Default\Control Panel\Accessibility\Keyboard Preference",On,%KeyboardpreferenceValue%
RegWrite,HKLM,0x1,"Tmp_Default\Control Panel\Accessibility\StickyKeys",Flags,%StickyKeysFlagsValue%
RegWrite,HKLM,0x1,"Tmp_Default\Control Panel\Accessibility\ToggleKeys",ToggleKeys,%ToggleKeysFlagsValue%
RegHiveUnLoad,Tmp_Default

