[Variables]
%ProgramTitle%=Task Manager
%ProgramEXE%=TaskMgr.exe
%ProgramFolder%=#$pSystemRoot#$p\system32

[Process]
If,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Echo,"Components PluginCache"
If,Not,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Run,%ScriptFile%,Process_Normal

[Process_Normal]
Require,File,Taskmgr.exe
//-
If,ExistSection,%Projectdir%\script.project,VariablesWimPack,Begin
//PEBakery Update - Later Disable %EngineVersion% lines here
If,ExistVar,%EngineVersion%,Echo,"PEBakery - AddVariables Macro Not implemented for now - Temporary Disable WimPackExclude",Warn
If,Not,ExistVar,%EngineVersion%,Begin
AddVariables,%Projectdir%\script.project,VariablesWimPack
WimPackExclude,"//Task Manager exclusion file list",TaskMgrExcludeList.txt
WimPackExclude,taskmgr.exe,TaskMgrExcludeList.txt
End
End
//-
If,Not,%pCheckBox1%%pCheckBox2%%pCheckBox3%,Equal,FalseFalseFalse,Run,%ScriptFile%,Shortcut

[Shortcut]
//Win10 "System Tools" [LocalizedFileNames] Task Manager.lnk=@%SystemRoot%\system32\Taskmgr.exe,-32420
Set,%DINI%,@taskmgr.exe#$c-10025
If,%OSFamily%,Equal,NT6,Set,%DINI%,@#$pSystemRoot#$p\system32\Taskmgr.exe#$c-32420
//-
If,%pCheckBox1%,Equal,True,Add_Shortcut,Desktop,,%ProgramFolder%\%ProgramEXE%,%ProgramTitle%
If,%pCheckBox1%,Equal,True,Add,DesktopIni,Desktop,,,%ProgramTitle%.lnk,%DINI%
If,%pCheckBox3%,Equal,True,Add_Shortcut,QuickLaunch,,%ProgramFolder%\%ProgramEXE%,%ProgramTitle%
If,%pCheckBox3%,Equal,True,Add,DesktopIni,QuickLaunch,,,%ProgramTitle%.lnk,%DINI%
If,%pCheckBox2%,Equal,True,Begin
Set,%Startmenu_Folder_Name%,""
If,%pRadioGroup1%,Equal,0,IniRead,%ProjectInfo%,String,Accessories,%Startmenu_Folder_Name%
If,"%Startmenu_Folder_Name%",Equal,"",Set,%Startmenu_Folder_Name%,Accessories
If,%pRadioGroup1%,Equal,1,Set,%Startmenu_Folder_Name%,%pTextBox1%
Add_Shortcut,StartMenu,%Startmenu_Folder_Name%,%ProgramFolder%\%ProgramEXE%,%ProgramTitle%
Add,DesktopIni,StartMenu,%Startmenu_Folder_Name%,,%ProgramTitle%.lnk,%DINI%
End

