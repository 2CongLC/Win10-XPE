[Variables]
%ProgramTitle%=
%ProgramEXE%=
%ProgramFolder%=

[Process]
If,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Echo,"Components PluginCache"
If,Not,ExistFile,%ProjectTemp%\Components_PluginCache_Extract.ini,Run,%ScriptFile%,Process_Normal

[Process_Normal]
IniRead,%ProjectDir%\script.project,IniVariables,NoWarnRegOverWrite,%NoWarnROW%
//--
//RegHiveLoad,Tmp_System,%RegSystem%
RegHiveLoad,Tmp_Default,%RegDefault%
//RegHiveLoad,Tmp_Software,%RegSoftware%
//--
If,%SM_DisableStartButtonPrompt_CheckBox%,Equal,True,Run,%ScriptFile%,DisableStartButtonPrompt
IF,%EnableBalloonTips_CheckBox%,Equal,True,Run,%ScriptFile%,EnableBalloonTips
If,%SM_RemoveSearch_CheckBox%,Equal,True,Run,%ScriptFile%,RemoveSearch
If,%SM_RemoveHelp_CheckBox%,Equal,True,Run,%ScriptFile%,RemoveHelp
If,%SM_RemoveDocs_CheckBox%,Equal,True,Run,%ScriptFile%,RemoveDocs
//-
//DragFullWindows Setting available at:
//Components\Tweaks\'Tweak Visual Effects'
//Gena\Components\Tweaks Explorer\'Tweaks and Settings'
//Added here since Win2003 it is default 0 - Some confort to Project Admin ;)
If,%Enable_CheckBox_DragFullWindows%,Equal,True,Begin
If,%NoWarnROW%,Equal,True,System,ErrorOff
RegWrite,HKLM,0x1,"Tmp_Default\Control Panel\Desktop",DragFullWindows,1
End
//--
//RegHiveUnLoad,Tmp_System
RegHiveUnLoad,Tmp_Default
//RegHiveUnLoad,Tmp_Software

[DisableStartButtonPrompt]
If,%NoWarnROW%,Equal,True,System,ErrorOff
RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced,StartButtonBalloonTip,2
//StartButtonBalloonTip not enough - NoSMballoonTip also required (Tested on XP x64)
If,%NoWarnROW%,Equal,True,System,ErrorOff
RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Policies\Explorer,NoSMballoonTip,1

[RemoveSearch]
If,%NoWarnROW%,Equal,True,System,ErrorOff
RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Policies\Explorer,NoFind,1

[RemoveHelp]
If,%NoWarnROW%,Equal,True,System,ErrorOff
RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Policies\Explorer,NoSMHelp,1
//enable: NoStartMenuHelp,0
If,%NoWarnROW%,Equal,True,System,ErrorOff
RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced,NoStartMenuHelp,1

[RemoveDocs]
If,%NoWarnROW%,Equal,True,System,ErrorOff
RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Policies\Explorer,NoSMMyDocs,1
//enable: NoRecentDocsMenu,1
If,%NoWarnROW%,Equal,True,System,ErrorOff
RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Policies\Explorer,NoRecentDocsMenu,1

[EnableBalloonTips]
//Right Down of Screen a red round white crossed (X) icon with a Baloon Tip:
//---
//Display Settings
//Your Computer's screen resolution and color depth are
//currently set to very low level. You can get a better picture
//by increasing these settings. To do this, click this balloon.
//---
//EnableBalloonTips,0 disables Balloon Tip still there is Red icon there to be clicked ;)
//--
//Tested: NoSMballoonTip,1 + StartButtonBalloonTip,2 - framebuf.dll --> you will get ballon --> you need EnableBalloonTips=0
//---
If,%NoWarnROW%,Equal,True,System,ErrorOff
RegWrite,HKLM,0x4,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Explorer\Advanced,EnableBalloonTips,0

