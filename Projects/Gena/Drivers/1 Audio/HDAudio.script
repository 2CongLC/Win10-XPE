[Process]
If,%SourceArch%,Equal,IA64,Exit,"HDAudio Plugin IA64 support not exists"
If,%SourceArch%,Equal,x64,Begin
Set,%pRadioButton1%,True
Set,%pRadioButton2%,False
Set,%pRadioButton3%,False
End
If,%SourceArch%,Equal,x86,Begin
If,%pRadioButton1%,EQUAL,True,If,%checkbox_autocheck%,Equal,True,Begin
Set,%foundini%,0
Set,%foundsys%,0
If,ExistFile,%Source_Sys%\HDAudBus.in?,Set,%foundini%,1
If,ExistFile,%Source_Sys%\HDAudBus.sy?,Set,%foundsys%,1
If,Not,%foundini%%foundsys%,Equal,11,Begin
Set,%foundini%,0
Set,%foundsys%,0
If,ExistFile,%pFileBox1%\HDAudBus.in?,Set,%foundini%,1
If,ExistFile,%pFileBox1%\HDAudBus.sy?,Set,%foundsys%,1
If,%foundini%%foundsys%,Equal,11,Set,%pRadioButton1%,False
If,%foundini%%foundsys%,Equal,11,Set,%pRadioButton2%,True
End
End
End
If,%pRadioButton1%,Equal,True,Run,%ScriptFile%,Process_HD-Audio,"%Source_Sys%","%Source_Sys%","%Source_Sys%"
If,%pRadioButton2%,Equal,True,Run,%ScriptFile%,Process_HD-Audio,"%pFileBox1%","%pFileBox1%","%pFileBox1%"
If,%pRadioButton3%,Equal,True,Begin
Call,HostOSArch
If,Not,%HostOSArch%,Equal,x86,Begin
Message,"Gathering of HDAudBus files is only supported under x86 architecture",ERROR,60
Run,%scriptfile%,EchoWarn,"Gathering of HDAudBus files is only supported under x86 architecture"
Exit,"Gathering of HDAudBus files is only supported under x86 architecture"
End
Else,Begin
Call,HostWindowsDir
Run,%ScriptFile%,Process_HD-Audio,%HostWindowsDir%\Inf,%HostWindowsDir%\system32\drivers,%HostWindowsDir%\system32
End
End

[Process_HD-Audio]
Echo,"Add HDAudio Bus driver"
If,%SourceArch%,Equal,x86,Begin
If,Not,ExistFile,#1\HDAudBus.in?,Message,"Error: file #$q#1\HDAudBus.in?#$q not found #$x Check Settings of %ScriptTitle% Plugin",Error,60
If,Not,ExistFile,#1\HDAudBus.in?,Run,%scriptfile%,EchoWarn,"Error: file #$q#1\HDAudBus.in?#$q not found -  Check Settings of %ScriptTitle% Plugin"
If,Not,ExistFile,#1\HDAudBus.in?,Exit,"Error: file #$q#1\HDAudBus.in?#$q not found"
If,Not,ExistFile,#1\HDAudBus.sy?,Message,"Error: file #$q#1\HDAudBus.in?#$q not found #$x Check Settings of %ScriptTitle% Plugin",Error,60
If,Not,ExistFile,#1\HDAudBus.sy?,Run,%scriptfile%,EchoWarn,"Error: file #$q#2\HDAudBus.sy?#$q not found - Check Settings of %ScriptTitle% Plugin"
If,Not,ExistFile,#2\HDAudBus.sy?,Exit,"Error: file #$q#2\HDAudBus.sy?#$q not found"
CopyOrExpand,#1\HDaudbus.inf,%Target_Win%\Inf
CopyOrExpand,#2\hdaudbus.sys,%Target_Sys%\Drivers
End
If,%SourceArch%,Equal,x64,Begin
CopyOrExpand,%source_win%\HDaudbus.inf,%Target_Win%\Inf
//hdaudbus.sys,%Source_Sys%\%spcabfile%
Require,HighVersion,hdaudbus.sys,TargetWin\System32\Drivers
End
//
RegHiveLoad,Tmp_System,%Target_Sys%\setupreg.hiv
RegWrite,HKLM,0x1,Tmp_System\ControlSet001\Control\CriticalDeviceDatabase\PCI#CC_0403,ClassGUID,{4D36E97D-E325-11CE-BFC1-08002BE10318}
RegWrite,HKLM,0x1,Tmp_System\ControlSet001\Control\CriticalDeviceDatabase\PCI#CC_0403,Service,HDAudBus
RegWrite,HKLM,0x4,Tmp_System\ControlSet001\Services\HDAudBus,Type,1
RegWrite,HKLM,0x4,Tmp_System\ControlSet001\Services\HDAudBus,Start,3
RegWrite,HKLM,0x1,Tmp_System\ControlSet001\Services\HDAudBus,Group,"Extended Base"
RegWrite,HKLM,0x1,Tmp_System\ControlSet001\Services\HDAudBus,DisplayName,"Microsoft UAA Bus Driver for High Definition Audio"
RegWrite,HKLM,0x2,Tmp_System\ControlSet001\Services\HDAudBus,ImagePath,system32\DRIVERS\hdaudbus.sys
RegWrite,HKLM,0x4,Tmp_System\ControlSet001\Services\HDAudBus,ErrorControl,0
RegHiveUnLoad,Tmp_System
//
// additional files
If,%SourceArch%,Equal,x86,Begin
If,ExistFile,#3\hdaprop.dl?,CopyOrExpand,#3\hdaprop.dll,%Target_Sys%
If,ExistFile,#3\hdashcut.ex?,CopyOrExpand,#3\hdashcut.exe,%Target_Sys%
If,ExistFile,#3\hdaudres.dl?,CopyOrExpand,#3\hdaudres.dll,%Target_Sys%
If,ExistFile,#2\portcls.sy?,CopyOrExpand,#2\portcls.sys,%Target_Sys%\drivers
End
If,%SourceArch%,Equal,x64,Begin
//hdaprop.dll,%Source_Sys%\%drcabfile%
//hdashcut.exe,%Source_Sys%\%drcabfile%
//hdaudres.dll,%Source_Sys%\%drcabfile%
//portcls.sys,%Source_Sys%\%spcabfile%
Require,HighVersion,hdaprop.dll,TargetWin\System32
Require,HighVersion,hdashcut.exe,TargetWin\System32
Require,HighVersion,hdaudres.dll,TargetWin\System32
Require,HighVersion,portcls.sys,TargetWin\System32\Drivers
End
//
//
Echo,"Add HD Audio device driver"
//
If,%CheckBox_hwpnp%,Equal,True,Begin
// PNP HD Audio device driver
//Run,%AutoRunScript%,Add,"HWPnp.exe -all +HDAUDIO"
If,Not,ExistFile,%Target_Sys%\HideRun.exe,Require,File,HideRun.exe
RegHiveLoad,Tmp_Software,%Target_Sys%\config\software
RegWrite,HKLM,0x1,Tmp_Software\Microsoft\Windows\CurrentVersion\RunOnceEx\998,HDAudio,"hiderun.exe /w HWPnp.exe -all +PCI\CC_04 +HDAUDIO /cid /d+ /log+"
RegHiveUnLoad,Tmp_Software
End
//
If,%pCheckBox1%,Equal,True,begin
If,ExistFile,#1\HDAudio.in?,CopyOrExpand,#1\HDAudio.inf,%Target_Win%\Inf
If,ExistFile,#2\HDAudio.sy?,CopyOrExpand,#2\HDAudio.sys,%Target_Sys%\Drivers
End
//
If,%pCheckBox2%,Equal,True,begin
ExtractAllFiles,%ScriptFile%,%SourceArch%_Inf,%Target_Win%\Inf
ExtractAllFiles,%ScriptFile%,%SourceArch%_Drivers,%Target_Sys%\Drivers
ExtractAllFiles,%ScriptFile%,%SourceArch%_System32,%Target_Sys%
End
//
If,%pCheckBox3%,Equal,True,begin
If,ExistFile,%GlobalTemplates%\drivers\HDAudio\%SourceArch%_inf\*.inf,FileCopy,%GlobalTemplates%\drivers\HDAudio\inf\*.inf,%Target_Win%\Inf
If,ExistFile,%GlobalTemplates%\drivers\HDAudio\%SourceArch%_drivers\*.sys,FileCopy,%GlobalTemplates%\drivers\HDAudio\drivers\*.*,%Target_Sys%\Drivers
If,ExistFile,%GlobalTemplates%\drivers\HDAudio\%SourceArch%_system32\*.dll,FileCopy,%GlobalTemplates%\drivers\HDAudio\system32\*.*,%Target_Sys%
End

[open_drivers_directorya]
Call,OpenDir,%pFileBox1%

[open_device_drivers]
If,Not,ExistDir,%GlobalTemplates%\drivers\HDAudio\x86_inf,DirMake,%GlobalTemplates%\drivers\HDAudio\x86_inf
If,Not,ExistDir,%GlobalTemplates%\drivers\HDAudio\x86_drivers,DirMake,%GlobalTemplates%\drivers\HDAudio\x86_drivers
If,Not,ExistDir,%GlobalTemplates%\drivers\HDAudio\x86_system32,DirMake,%GlobalTemplates%\drivers\HDAudio\x86_system32
If,Not,ExistDir,%GlobalTemplates%\drivers\HDAudio\x64_inf,DirMake,%GlobalTemplates%\drivers\HDAudio\x64_inf
If,Not,ExistDir,%GlobalTemplates%\drivers\HDAudio\x64_drivers,DirMake,%GlobalTemplates%\drivers\HDAudio\x64_drivers
If,Not,ExistDir,%GlobalTemplates%\drivers\HDAudio\x64_system32,DirMake,%GlobalTemplates%\drivers\HDAudio\x64_system32
Call,OpenDir,%GlobalTemplates%\drivers\HDAudio

