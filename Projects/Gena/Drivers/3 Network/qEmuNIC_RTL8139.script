[Remove_From_Build]
If,ExistFile,%Target_Win%\inf\NETRTSNT.INf,FileDelete,%Target_Win%\inf\NETRTSNT.INf
If,ExistFile,%Target_Sys%\drivers\rtl8139.sys,FileDelete,%Target_Sys%\drivers\rtl8139.sys
If,ExistFile,%Target_Win%\inf\NETRTSNX.INf,FileDelete,%Target_Win%\inf\NETRTSNX.INf
If,ExistFile,%Target_Sys%\drivers\rtl39a64.sys,FileDelete,%Target_Sys%\drivers\rtl39a64.sys

[Process]
Run,%ScriptFile%,Process_%SourceArch%

[Process_x86]
If,%OSFamily%,Equal,NT5,Begin
CopyOrExpand,%Source_Win%\NETRTSNT.INF,%Target_Win%\inf
Require,HighVersion,rtl8139.sys,TargetWin\System32\Drivers
End
Else,Begin
// I do not know the required lines for Nt6x - support needed
End

[Process_x64]
If,%OSFamily%,Equal,NT5,Begin
CopyOrExpand,%Source_Win%\NETRTSNX.INF,%Target_Win%\inf
Require,HighVersion,rtl39a64.sys,TargetWin\System32\Drivers
End
Else,Begin
// I do not know the required lines for Nt6x - support needed
End

[Process_IA64]
Echo,"No pxe-rtl8139.bin support added",WARN

