[numbercheck]
set,%numbercheckloop%,no
If,%number%,smaller,10000,set,%numbertest%,%number%
If,%number%,smaller,1000,set,%numbertest%,0%number%
If,%number%,smaller,100,set,%numbertest%,00%number%
If,%number%,smaller,10,set,%numbertest%,000%number%
RegHiveLoad,Tmp_System,%RegSystem%
//If,ExistRegSection,HKLM,Tmp_System\ControlSet001\Control\Class\{4D36E968-E325-11CE-BFC1-08002BE10318}\%numbertest%,set,%found%,yes
If,ExistRegKey,HKLM,Tmp_System\ControlSet001\Control\Class\{4D36E968-E325-11CE-BFC1-08002BE10318}\%numbertest%,InfPath,Begin
RegRead,HKLM,Tmp_System\ControlSet001\Control\Class\{4D36E968-E325-11CE-BFC1-08002BE10318}\%numbertest%,InfPath,%verification%
If,%verification%,Equal,display.inf,set,%numbercheckloop%,no
If,Not,%verification%,Equal,display.inf,set,%numbercheckloop%,yes
End
RegHiveUnLoad,Tmp_System
set,%number%,%numbertest%
If,%numbercheckloop%,Equal,yes,StrFormat,INC,%number%,1
If,%numbercheckloop%,Equal,yes,run,%scriptfile%,numbercheck

[Process]
If,Not,%SourceArch%,Equal,x86,Exit,"QemuVGA Plugin only for x86 builds"
CopyOrExpand,%source_win%\display.inf,%target_win%\inf
Require,HighVersion,cirrus.dll,TargetWin\System32
Require,HighVersion,cirrus.sys,TargetWin\System32\Drivers
If,%pCheckBox1%,Equal,True,Run,%ScriptFile%,registryadds
RegHiveLoad,Tmp_System,%RegSystem%
If,%pScrollBox1%,Equal,0,If,ExistRegKey,HKLM,Tmp_System\ControlSet001\Services\cirrus\Device0,Acceleration.Level,RegDelete,HKLM,Tmp_System\ControlSet001\Services\cirrus\Device0,Acceleration.Level
If,Not,%pScrollBox1%,Equal,0,RegWrite,HKLM,0x4,Tmp_System\ControlSet001\Services\cirrus\Device0,Acceleration.Level,%pScrollBox1%
RegHiveUnLoad,Tmp_System

