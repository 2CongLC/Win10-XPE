[Delete_SYS_M]
//Par1: FileName / Par2: Registry Key / Par3: TxtSetup.sif Section / Par4: TxtSetup.sif Section Entry
Call,FileDelete,%Target_Sys%\Drivers\#1.SYS
Call,FileDelete,%Target_Sys%\Drivers\#1.SY_
//-
If,Not,#2-,Equal,-,Begin
If,ExistRegSection,HKLM,Tmp_System\ControlSet001\Services\#2,RegDelete,HKLM,Tmp_System\ControlSet001\Services\#2
If,ExistRegSection,HKLM,Tmp_System\ControlSet001\Services\Eventlog\System\#2,RegDelete,HKLM,Tmp_System\ControlSet001\Services\Eventlog\System\#2
End
//-
If,Not,#3-,Equal,-,Begin
If,ExistFile,%Target_Win%\TXTSETUP.SIF,IniDelete,%Target_Win%\TXTSETUP.SIF,#3,#4
End

