[Process_Common]
If,Not,ExistDir,%ProjectTemp%,DirMake,%ProjectTemp%
StrFormat,DATE,%myDate%,yyyymmddhhnn
//Backup PreConfig For Reference-Tests....
ShellExecute,hide,regedit.exe,"/e #$q%ProjectTemp%\Keyboard_Layout-Preload_%myDate%.reg.ini#$q #$qHKEY_LOCAL_MACHINE\Tmp_Default\Keyboard Layout\Preload#$q"
