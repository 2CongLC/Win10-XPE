[Variables]

[Process]
//--
//you can not revert back for now - main language \Font\ files comes from core build or a language special plugin - you must rebuild without this option
If,%Font_Del_ALL_CheckBox%,Equal,True,Call,DirDeleteMake,%Target_Win%\Fonts
//--
//marlett.ttf - main symbols - ex: ( X (Close) _ (Minimize) buttons at title)
If,%marlett_ttf_CheckBox%,Equal,True,Require,Font,marlett.ttf
//arial.ttf - Required by Explorer - without Explorer have no text
If,%arial_ttf_CheckBox%,Equal,True,Require,Font,arial.ttf
//--
If,%NLS_Dell_KDB_DLL_CheckBox%,Equal,True,Run,%ScriptFile%,Delete_ALL_KBD_DLL
//--
If,%NLS_Dell_C_NLS_CheckBox%,Equal,True,Run,%ScriptFile%,Delete_ALL_C_NLS
//--
If,%Add_ALL_CheckBox%,Equal,True,Run,%ScriptFile%,Add_ALL
//--
If,ExistFile,%Target_Win%\TxtSetup.sif,Begin
If,%NLS_Mandatory_CheckBox%,Equal,True,Run,%ScriptFile%,TxtSetup_NLS_ADD_MANDATORY
If,%NLS_Compress_CheckBox%,Equal,True,Run,%ScriptFile%,TxtSetup_NLS_Compress
End
//--

[Add_ALL]
If,%Add_ALL_NLS_CheckBox%,Equal,True,Run,%ScriptFile%,Add_ALL_NLS
If,%Add_ALL_TTF_CheckBox%,Equal,True,Run,%ScriptFile%,Add_ALL_TTF
If,%Add_ALL_FON_CheckBox%,Equal,True,Run,%ScriptFile%,Add_ALL_FON

[Add_ALL_NLS]
If,%OSFamily%,Equal,NT5,Run,%ScriptFile%,Add_ALL_NLS_NT5
Else,Begin
FileCopy,%Source_Win%\*.nls,%Target_Sys%
End

[Add_ALL_NLS_NT5]
Call,DirDeleteMake,%ProjectTemp%\FontsFromSource
StrFormat,SHORTPATH,%Source_Win%,%Source_WinFolderShortpath%
//--
Call,DirDeleteMake,%ProjectTemp%\NLSFromSource
ShellExecute,hide,expand," -r %Source_WinFolderShortpath%\*.nl_ #$q%ProjectTemp%\NLSFromSource#$q"
FileCopy,%ProjectTemp%\NLSFromSource\*.nls,%Target_Sys%
FileCopy,%Source_Win%\*.nls,%Target_Sys%

[Add_ALL_TTF]
If,%OSFamily%,Equal,NT5,Run,%ScriptFile%,Add_ALL_TTF_NT5
Else,Begin
FileCopy,%Source_Win%\Fonts\*.ttf,%Target_Win%\Fonts
End

[Add_ALL_TTF_NT5]
Call,DirDeleteMake,%ProjectTemp%\FontsFromSource
StrFormat,SHORTPATH,%Source_Win%,%Source_WinFolderShortpath%
//--
ShellExecute,hide,expand," -r %Source_WinFolderShortpath%\*.tt_ #$q%ProjectTemp%\FontsFromSource#$q"
FileCopy,%ProjectTemp%\FontsFromSource\*.ttf,%Target_Win%\Fonts
FileCopy,%Source_Win%\*.ttf,%Target_Win%\Fonts

[Add_ALL_FON]
If,%OSFamily%,Equal,NT5,Run,%ScriptFile%,Add_ALL_FON_NT5
Else,Begin
FileCopy,%Source_Win%\Fonts\*.fon,%Target_Win%\Fonts
End

[Add_ALL_FON_NT5]
Call,DirDeleteMake,%ProjectTemp%\FontsFromSource
StrFormat,SHORTPATH,%Source_Win%,%Source_WinFolderShortpath%
//--
ShellExecute,hide,expand," -r %Source_WinFolderShortpath%\*.fo_ #$q%ProjectTemp%\FontsFromSource#$q"
FileCopy,%ProjectTemp%\FontsFromSource\*.fon,%Target_Win%\Fonts
FileCopy,%Source_Win%\*.fon,%Target_Win%\Fonts

[Delete_ALL_KBD_DLL]
//kbdxxxx.dl_ should not exists ;) ps: keyboard files are not compressable - if compressed they simply do not work
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%target_sys%\kbd*.dll#$q&del /q /f /A:R /A:H /A:S /A:A #$q%target_sys%\kbd*.dll#$q"
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%target_sys%\kbd*.dl_#$q&del /q /f /A:R /A:H /A:S /A:A #$q%target_sys%\kbd*.dl_#$q"

[Delete_ALL_C_NLS]
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%target_sys%\C_*.NLS#$q&del /q /f /A:R /A:H /A:S /A:A #$q%target_sys%\C_*.NLS#$q"
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%target_sys%\C_*.NL_#$q&del /q /f /A:R /A:H /A:S /A:A #$q%target_sys%\C_*.NL_#$q"

[TxtSetup_NLS_ADD_MANDATORY]
//Adds first values of [nls] Mandatory
Call,DirMake,%ProjectTemp%
Call,FileDelete,%ProjectTemp%\TxtSetup_Processed.sif
FileCopy,%Target_Win%\TxtSetup.sif,%ProjectTemp%\TxtSetup_Processed.sif
TxtReplace,%ProjectTemp%\TxtSetup_Processed.sif,",",|
//-
//-
//AnsiCodepage=c_1252.nls,1252
//OemCodepage=c_437.nls,437,c_850.nls,850
//MacCodepage=c_10000.nls,10000
//UnicodeCasetable=l_intl.nls,0409
//OemHalFont=vgaoem.fon
//-
Run,%ScriptFile%,TxtSetup_NLS_Copy_M,%ProjectTemp%\TxtSetup_Processed.sif,nls,AnsiCodepage
Run,%ScriptFile%,TxtSetup_NLS_Copy_M,%ProjectTemp%\TxtSetup_Processed.sif,nls,OemCodepage
//not mandatory
//Run,%ScriptFile%,TxtSetup_NLS_Copy_M,%ProjectTemp%\TxtSetup_Processed.sif,nls,MacCodepage
Run,%ScriptFile%,TxtSetup_NLS_Copy_M,%ProjectTemp%\TxtSetup_Processed.sif,nls,UnicodeCasetable
Run,%ScriptFile%,TxtSetup_NLS_Copy_M,%ProjectTemp%\TxtSetup_Processed.sif,nls,OemHalFont

[TxtSetup_NLS_Copy_M]
IniRead,#1,#2,#3,#4
StrFormat,Split,#4,|,0,#8
//Loop,%ScriptFile%,Compress_M_Loop,1,#8,#1,#2,#3,#4
//here loop 1 to 1 to only get first value
Loop,%ScriptFile%,TxtSetup_NLS_Copy_M_Loop,1,1,#1,#2,#3,#4

[TxtSetup_NLS_Copy_M_Loop]
StrFormat,Split,#4,|,#c,#8
//Echo,#8,Warn
StrFormat,Ext,#8,#7
//Echo,#7,Warn
If,#7,Equal,.nls,Begin
StrFormat,RTRIM,#8,4,#6
StrFormat,UCASE,#6,#6
//Echo,#6,Warn
Call,FileDelete,%Target_Sys%\#6.NLS
Call,FileDelete,%Target_Sys%\#6.NL_
CopyOrExpand,%Source_Sys%\#6.NLS,%Target_Sys%\#6.NLS
End
If,#7,Equal,.fon,Begin
StrFormat,RTRIM,#8,4,#6
StrFormat,UCASE,#6,#6
//Echo,#6,Warn
Call,FileDelete,%Target_Sys%\#6.FON
Call,FileDelete,%Target_Sys%\#6.FO_
CopyOrExpand,%Source_Sys%\#6.FON,%Target_Sys%\#6.FON
End

[TxtSetup_NLS_Compress]
Call,DirMake,%ProjectTemp%
Call,FileDelete,%ProjectTemp%\TxtSetup_Processed.sif
FileCopy,%Target_Win%\TxtSetup.sif,%ProjectTemp%\TxtSetup_Processed.sif
TxtReplace,%ProjectTemp%\TxtSetup_Processed.sif,",",|
//-
//AnsiCodepage=c_1252.nls,1252
//OemCodepage=c_437.nls,437,c_850.nls,850
//MacCodepage=c_10000.nls,10000
//UnicodeCasetable=l_intl.nls,0409
//OemHalFont=vgaoem.fon
//-
//Run,%ScriptFile%,Compress,%Target_Sys%\C_1252,NLS,NL_
//Run,%ScriptFile%,Compress,%Target_Sys%\c_437,NLS,NL_
//Run,%ScriptFile%,Compress,%Target_Sys%\c_10000,NLS,NL_
//Run,%ScriptFile%,Compress,%Target_Sys%\L_INTL,NLS,NL_
//Run,%ScriptFile%,Compress,%Target_Sys%\VGAOEM,FON,FO_
//-
//AnsiCodepage File MANDATORY (value loaded from TxtSetup.sif)
Run,%ScriptFile%,Compress_M,%ProjectTemp%\TxtSetup_Processed.sif,nls,AnsiCodepage
//OemCodepage File MANDATORY (value loaded from TxtSetup.sif)
//OemCodepage 2nd value generally not exists (ex: enu c_850.nls ) not mandatory
Run,%ScriptFile%,Compress_M,%ProjectTemp%\TxtSetup_Processed.sif,nls,OemCodepage
//MacCodepage File Generally not exists
Run,%ScriptFile%,Compress_M,%ProjectTemp%\TxtSetup_Processed.sif,nls,MacCodepage
//UnicodeCasetable File MANDATORY (value loaded from TxtSetup.sif)
Run,%ScriptFile%,Compress_M,%ProjectTemp%\TxtSetup_Processed.sif,nls,UnicodeCasetable
//OemHalFont File MANDATORY (value loaded from TxtSetup.sif)
Run,%ScriptFile%,Compress_M,%ProjectTemp%\TxtSetup_Processed.sif,nls,OemHalFont

[Compress_M]
IniRead,#1,#2,#3,#4
//Echo,#4,Warn
StrFormat,Split,#4,|,0,#8
//Echo,#8,Warn
Loop,%ScriptFile%,Compress_M_Loop,1,#8,#1,#2,#3,#4

[Compress_M_Loop]
StrFormat,Split,#4,|,#c,#8
//Echo,#8,Warn
StrFormat,Ext,#8,#7
//Echo,#7,Warn
If,#7,Equal,.nls,Begin
StrFormat,RTRIM,#8,4,#6
StrFormat,UCASE,#6,#6
//Echo,#6,Warn
Run,%ScriptFile%,Compress,%Target_Sys%\#6,NLS,NL_
End
If,#7,Equal,.fon,Begin
StrFormat,RTRIM,#8,4,#6
StrFormat,UCASE,#6,#6
//Echo,#6,Warn
Run,%ScriptFile%,Compress,%Target_Sys%\#6,FON,FO_
End

[Compress]
//#1 Folder\Filename #2 FileExtension #3 FileTargetExtension
If,ExistFile,#1.#2,Begin
//-
If,ExistFile,#1.#3,ShellExecute,Hide,attrib.exe,"-R -S -A -H #$q#1.#3#$q"
If,ExistFile,#1.#3,ShellExecute,Hide,cmd.exe,"/C del /q /f #$q#1.#3#$q&del /q /f /A:R /A:H /A:S /A:A #$q#1.#3#$q"
If,ExistFile,#1.#3,Echo,"File NOT DELETED",WARN
//-
If,Not,ExistFile,#1.#3,Begin
ShellExecute,Hide,attrib.exe,"-R -S -A -H #$q#1.#2#$q"
ShellExecute,Hide,makecab.exe,"/D CompressionType=LZX /D CompressionMemory=21 /D Cabinet=ON /D Compress=ON /D FolderSizeThreshold=5000000 #$q#1.#2#$q #$q#1.#3#$q"
If,ExistFile,#1.#3,Begin
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q#1.#2#$q&del /q /f /A:R /A:H /A:S /A:A #$q#1.#2#$q"
If,ExistFile,#1.#2,Echo,"File NOT DELETED",WARN
End
Else,Echo,"#1.#2 NOT Compressed to #1.#3",WARN
End
Else,Echo,"#1.#3 File NOT DELETED - NO Compression Made",WARN
End
Else,Echo,"#1.#2 NOT FOUND"
//E-lse,Echo,"#1.#2 NOT FOUND",WARN

