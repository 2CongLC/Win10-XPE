[Variables]
%gsar%=%Tools%\gsar.exe
%hash%=#

[Process]
If,Not,%OSFamily%,Equal,NT5,Exit,"%ScriptTitle% Plugin only for Gena"
If,Not,%SourceArch%,Equal,x86,Exit,"%ScriptTitle% no work done for Gena x64 yet"
IniRead,%ProjectDir%\script.project,IniVariables,spCabFile,%spCabFile%
If,Not,ExistFile,%Source_Sys%\%spCabFile%,Exit,"%ScriptTitle% designed to work only with spcab file"
//--
If,%pRadioButton2%,Equal,True,Run,%ScriptFile%,CheckBootSector
//--
If,Not,ExistFile,%gsar%,ExtractFile,%ScriptFile%,Folder,gsar.exe,%Tools%
If,%pRadioButton1%,Equal,True,Run,%ScriptFile%,multiprocessor_only
If,%pRadioButton2%,Equal,True,Run,%ScriptFile%,both_processors
If,Not,%pRadioButton3%,Equal,True,Begin
If,ExistSection,%ProjectDir%\script.project,VariablesWimPack,Begin
AddVariables,%ProjectDir%\script.project,VariablesWimPack
WimPackExclude,"//Multiprocessor exclusion file list",MultiprocessorExcludeList.txt
WimPackExclude,ntkrnlmp.ex?,MultiprocessorExcludeList.txt
End
End
If,%pCheckBox1%,Equal,True,Run,%ScriptFile%,AddProcessorDrivers

[CheckBootSector]
Run,%ProjectDir%\script.project,SetOtherOS
Echo,%BootSector%
If,Not,%BootSector%,Equal,Grub4Dos,Exit,"%ScriptTitle% - 'Grub4dos menu Selection' requires 'Grub4dos BootSector' enabled on BootsectorPE Plugin"

[AddProcessorDrivers]
ShellExecute,Hide,expand.exe,"#$q%Source_Sys%\%spCabFile%#$q -F:intelppm.sys #$q%Target_Sys%\drivers#$q"
ShellExecute,Hide,expand.exe,"#$q%Source_Sys%\%spCabFile%#$q -F:processr.sys #$q%Target_Sys%\drivers#$q"
ShellExecute,Hide,expand.exe,"#$q%Source_Sys%\%spCabFile%#$q -F:p3.sys #$q%Target_Sys%\drivers#$q"
ShellExecute,Hide,expand.exe,"#$q%Source_Sys%\%spCabFile%#$q -F:amdk6.sys #$q%Target_Sys%\drivers#$q"
ShellExecute,Hide,expand.exe,"#$q%Source_Sys%\%spCabFile%#$q -F:amdk7.sys #$q%Target_Sys%\drivers#$q"
If,ExistFile,%pFileBox1%,FileCopy,%pFileBox1%,%Target_Sys%\drivers
ShellExecute,Hide,expand.exe,"#$q%Source_Sys%\%spCabFile%#$q -F:amdk8.sys #$q%Target_Sys%\drivers#$q"
//
// Intel:
// All Family 5 processors run processr.sys
// Family 6
// Model 0-7, 10, 12 --> processr.sys
// Model 8, 11 --> p3.sys
// Model 9, 13, all newer --> intelppm.sys
// All P4's (except model 0 & 1) and newer --> intelppm.sys
