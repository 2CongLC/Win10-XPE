[Verification-Gena]
Echo,"Verification: Performing verifications on %ScriptTitle%"
IniRead,%VerificationInfo%,"%ScriptTitle%_Main",Selected,%ScriptSel%
If,%ScriptSel%,Equal,True,Begin
IniRead,%VerificationInfo%,BootsectorPE_Main,Selected,%Sel%
If,Not,%Sel%,Equal,True,Begin
Message,"Verification error: Multi Processor Plugin requires BootsectorPE Plugin",ERROR
Halt,"Verification error: Multi Processor Plugin requires BootsectorPE Plugin"
End
IniRead,%VerificationInfo%,"Multi Processor_Interface",pRadioButton2,%Int%
If,%Int%,Equal,True,Begin
IniRead,%VerificationInfo%,"BootsectorPE_Interface",pRadioButton2,%Int%
If,Not,%Int%,Equal,True,Begin
Message,"Verification error: Multi Processor Plugin requires grub4dos bootsector enabled on BootsectorPE Plugin",ERROR
Halt,"Verification error: Multi Processor Plugin requires grub4dos bootsector enabled on BootsectorPE Plugin"
End
End
IniRead,%VerificationInfo%,Win2k3_Ramdisk_Main,Selected,%Sel%
If,Not,%Sel%,Equal,True,Begin
IniRead,%VerificationInfo%,WimBoot_Main,Selected,%Sel%
If,Not,%Sel%,Equal,True,Begin
Message,"Verification error: Multi Processor Plugin requires W2k3SP1 SETUPLDR.BIN. Try enabling the Win2k3_Ramdisk plugin.",ERROR
Halt,"Verification error: Multi Processor Plugin requires W2k3SP1 SETUPLDR.BIN. Try enabling the Win2k3_Ramdisk plugin."
End
End
End

