[Process_Normal]
Echo,"EnvironmentPE: Setting User Profile options..."
//--
//- winbuilder bug Set,%UserProfileSelect%,%pUserProfileScrollBox%
//- IniRead,%ScriptFile%,Main,Interface,%t%
//- If,%t%,Equal,InterfaceBasic,Begi-n
//  IniWrite,%ScriptFile%,Variables_Interface,#$ppUserProfileScrollBox#$p,%UserProfileSelect%
//  AddVariables,%ScriptFile%,Variables_Interface
//En-d
//--
IniRead,%ScriptFile%,Main,Interface,%t%
ChangeInterface,Read,Value,,%t%,pUserProfileScrollBox,%UserProfileSelect%
IniWrite,%ScriptFile%,Variables_Interface,#$ppUserProfileScrollBox#$p,%UserProfileSelect%
AddVariables,%ScriptFile%,Variables_Interface
//--
IniRead,%ProjectDir%\script.project,IniVariables,PrepPEGenScript,%PrepPEGenScript%
//Set,%RAMDriveLetter%,--,PERMANENT
Set,%RAMDriveLetter%,--
IniWrite,%ProjectDir%\script.project,IniVariables,RegTyp,0x1
If,%UserProfileSelect%,Equal,"System Drive (with packuser.exe)",IniWrite,%ProjectDir%\script.project,IniVariables,RegTyp,0x2
If,%UserProfileSelect%,Equal,"System Drive (no packuser.exe)",IniWrite,%ProjectDir%\script.project,IniVariables,RegTyp,0x2
If,%UserProfileSelect%,Equal,"System Drive (with packuser.exe)",Begin
Set,%SettingsDrive%,##$pSystemDrive##$p,PERMANENT
If,ExistFile,%profilesetting%,Begin
IniRead,%profilesetting%,Interface,ProfilePack_CheckBox,"%ProfilePack_CheckBox%"
StrFormat,REPLACE,"%ProfilePack_CheckBox%",False,True,"%ProfilePack_CheckBox%"
IniWrite,%profilesetting%,Interface,ProfilePack_CheckBox,"%ProfilePack_CheckBox%"
End
End
If,%UserProfileSelect%,Equal,"System Drive (no packuser.exe)",Begin
Set,%SettingsDrive%,##$pSystemDrive##$p,PERMANENT
//Set,%RAMDriveLetter%,##$pSystemDrive##$p,PERMANENT
Set,%RAMDriveLetter%,##$pSystemDrive##$p
If,ExistFile,%profilesetting%,Begin
IniRead,%profilesetting%,Interface,ProfilePack_CheckBox,"%ProfilePack_CheckBox%"
StrFormat,REPLACE,"%ProfilePack_CheckBox%",True,False,"%ProfilePack_CheckBox%"
IniWrite,%profilesetting%,Interface,ProfilePack_CheckBox,"%ProfilePack_CheckBox%"
End
End
If,%UserProfileSelect%,Equal,"RAM Drive",Set,%SettingsDrive%,"RAMDrive",PERMANENT
If,%Temp_Custom_CheckBox%,Equal,True,Begin
If,%Temp_Custom_RadioButtonPT%,Equal,True,Begin
End
If,%Temp_Custom_RadioButtonCS%,Equal,True,Begin
StrFormat,REPLACE,%Temp_Custom_Drive_ScrollBox%," ","",%Temp_Custom_Drive%
//Set,%RAMDriveLetter%,%Temp_Custom_Drive%,PERMANENT
Set,%RAMDriveLetter%,%Temp_Custom_Drive%
End
End
IniWrite,%ProjectInfo%,RamDrive,Settings,%SettingsDrive%
IniWrite,%ProjectDir%\script.project,IniVariables,RAMDriveLetter,%RAMDriveLetter%
//Run,%ScriptFile%,TempPE
TXTAddLine,%PrepPEGenScript%,"Exec#$c%ScriptFile%#$cTempPE",APPEND

[TempPE]
Echo,"EnvironmentPE: Setting temp environment variable options..."
//--
Set,%UserProfileSelect%,%pUserProfileScrollBox%
IniRead,%ScriptFile%,Main,Interface,%t%
If,%t%,Equal,InterfaceBasic,AddVariables,%ScriptFile%,Variables_Interface
//If,%t%,Equal,InterfaceBasic,Call,AddInterface,%ScriptFile%,Interface
//--
If,Not,%Temp_Custom_CheckBox%,Equal,True,Begin
If,%UserProfileSelect%,Equal,"System Drive (with packuser.exe)",Set,%TD%,#$pSystemDrive#$p
If,%UserProfileSelect%,Equal,"System Drive (no packuser.exe)",Set,%TD%,#$pSystemDrive#$p
If,%UserProfileSelect%,Equal,"RAM Drive",Set,%TD%,%SettingsDrive%
IniRead,%ProjectInfo%,String,ProfileBase,%docs%
IniRead,%ProjectInfo%,String,DefaultUser,%user%
IniRead,%ProjectInfo%,String,LocalSettings,%settings%
IniRead,%ProjectInfo%,String,Temp,%sttmp%
Set,%TempFolder%,%docs%\%user%\%settings%\%sttmp%
DirMake,%TargetProfilesBaseDir%\%TempFolder%
Set,%tmp%,%TD%\%TempFolder%
Set,%tmp%,#$pUSERPROFILE#$p\%settings%\Temp
End
Else,Begin
If,%Temp_Custom_RadioButtonCS%,Equal,True,Begin
StrFormat,LEFT,%Temp_Custom_Drive_ScrollBox%,2,%Var%
StrFormat,LTrim,%Var%,1,%Var%
If,Not,%Var%,Equal,:,Begin
If,%Temp_Custom_Drive_ScrollBox%,Equal,SettingsDrive,Set,%TD%,%SettingsDrive%
If,%Temp_Custom_Drive_ScrollBox%,Equal,"RAM Drive",Begin
IniWrite,%ProjectDir%\script.project,IniVariables,RAMDriveLetter,%RAMDriveLetter%
Set,%TD%,%RAMDriveLetter%
End
If,%Temp_Custom_Drive_ScrollBox%,Equal,"System Drive",Set,%TD%,##$pSystemDrive##$p
DirMake,%TargetProfilesBaseDir%\%Temp_Custom_Folder_TextBox%
End
Else,Set,%TD%,%Temp_Custom_Drive_ScrollBox%
Set,%tmp%,%TD%\%Temp_Custom_Folder_TextBox%
End
If,%Temp_Custom_RadioButtonPT%,Equal,True,Begin
Echo,%UserProfileSelect%
If,%UserProfileSelect%,Equal,"System Drive (with packuser.exe)",Set,%TD%,#$pSystemDrive#$p
If,%UserProfileSelect%,Equal,"System Drive (no packuser.exe)",Set,%TD%,#$pSystemDrive#$p
If,%UserProfileSelect%,Equal,"RAM Drive",Set,%TD%,%SettingsDrive%
IniRead,%ProjectInfo%,String,ProfileBase,%docs%
Set,%TempFolder%,%docs%\Temp
DirMake,%TargetProfilesBaseDir%\%TempFolder%
Set,%tmp%,%TD%\%TempFolder%
End
End
//--
If,Not,ExistFile,%Target_Sys%\HideRun.exe,Require,File,HideRun.exe
RegHiveLoad,Tmp_Default,%RegDefault%
RegHiveLoad,Tmp_Software,%Target_Sys%\config\software
If,Not,%UserProfileSelect%,Equal,"System Drive",RegWrite,HKLM,0x1,Tmp_Software\Microsoft\Windows\CurrentVersion\RunOnceEx\000,imdisktmp,"hiderun.exe /w cmd.exe /c md #$q%tmp%#$q"
If,%pBroadcastEnvChangeCheckBox%,Equal,True,RegWrite,HKLM,0x1,Tmp_Software\Microsoft\Windows\CurrentVersion\Run,BroadcastEnvChange,"hiderun.exe /w BroadcastEnvChange.exe /broadcast"
//If,%pBroadcastEnvChangeCheckBox%,Equal,True,RegWrite,HKLM,0x1,Tmp_Default\Software\Microsoft\Windows\CurrentVersion\Run,BroadcastEnvChange,"hiderun.exe /w BroadcastEnvChange.exe /broadcast"
Require,File,BroadcastEnvChange.exe
RegHiveUnLoad,Tmp_Software
RegHiveUnLoad,Tmp_Default
RegHiveLoad,Tmp_System,%Target_Sys%\setupreg.hiv
RegWrite,HKLM,0x2,"Tmp_System\ControlSet001\Control\Session Manager\Environment",Temp,%tmp%
RegWrite,HKLM,0x2,"Tmp_System\ControlSet001\Control\Session Manager\Environment",Tmp,%tmp%
RegHiveUnLoad,Tmp_System
RegHiveLoad,Tmp_Default,%Target_Sys%\config\default
If,ExistRegKey,HKLM,Tmp_Default\Environment,Temp,RegDelete,HKLM,Tmp_Default\Environment,Temp
If,ExistRegKey,HKLM,Tmp_Default\Environment,Tmp,RegDelete,HKLM,Tmp_Default\Environment,Tmp
RegWrite,HKLM,0x2,Tmp_Default\Environment,Temp,%tmp%
RegWrite,HKLM,0x2,Tmp_Default\Environment,Tmp,%tmp%
RegHiveUnLoad,Tmp_Default

