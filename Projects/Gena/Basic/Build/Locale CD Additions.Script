[Process]
If,ExistFile,%ProjectTemp%\Basic_PluginCache_Extract.ini,Echo,"Basic PluginCache"
If,Not,ExistFile,%ProjectTemp%\Basic_PluginCache_Extract.ini,Run,%ScriptFile%,Process_Normal

[Process_Normal]
IniRead,%ProjectDir%\script.project,IniVariables,SourceLocale,%SourceLocale%
If,Not,%SourceLocale%-,Equal,-,Begin
If,ExistSection,%ScriptFile%,Process%SourceLocale%,Run,%ScriptFile%,Process%SourceLocale%
End

[c_1252nls]
//not used by script, kept for future reference
//c_1252.nls is NOT required by windows (PE) (no report so far), it is required by 3rd party applications (HdTune, ultraiso, etc.), since dependency not based on targetpe, this is not the script to add this file.
If,Not,ExistFile,%Target_Sys%\c_1252.nls,CopyOrExpand,%Source_Sys%\c_1252.nls,%Target_Sys%

[arialbdttf]
If,Not,ExistFile,%Target_Win%\Fonts\arialbd.ttf,CopyOrExpand,%Source_Win%\arialbd.ttf,%Target_Win%\Fonts

[Info]
// (English (and some other western sources) AnsiCodepage=c_1252.nls)

