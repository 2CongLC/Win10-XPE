IniRead,%ProjectInfo%,String,AdministrativeTools,%StartMenuFolder%
Add_Shortcut,StartMenu,%StartMenuFolder%,"#$pSystemRoot#$p\system32\#1",%MscTitleAppTitleVar%
Run,%scriptfile%,CreateMMCShortcuts_DesktopIni,#1
End

