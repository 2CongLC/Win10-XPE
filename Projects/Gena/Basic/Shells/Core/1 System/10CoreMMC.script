//Chinese - Singapore
If,%t%,Equal,4100,Set,%Extended%,Yes
//Chinese - Taiwan
If,%t%,Equal,1028,Set,%Extended%,Yes
//Chinese - Hong Kong SAR
If,%t%,Equal,3076,Set,%Extended%,Yes
//Chinese - Macao SAR
If,%t%,Equal,5124,Set,%Extended%,Yes
//Japanese
If,%t%,Equal,1041,Set,%Extended%,Yes
//Korean
