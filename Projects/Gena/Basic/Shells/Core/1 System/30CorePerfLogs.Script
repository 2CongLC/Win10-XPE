[postproc]
Echo,"Postprocessing: Performance Logs and Alerts ..."
IniRead,%ProjectDir%\script.project,IniVariables,NoWarnRegOverWrite,%NoWarnROW%
RegHiveLoad,Tmp_System,%Target_Sys%\setupreg.hiv
Set,%TMP%,""
System,ERROROFF
RegRead,HKLM,Tmp_System\ControlSet001\Services\SysmonLog,DisplayName,%TMP%
If,%TMP%,Equal,"",Set,%TMP%,"Performance Logs and Alerts"
//RegWrite ! Overwrote existing key "%SystemDrive%\PerfLogs" with different data
If,%NoWarnROW%,Equal,True,System,ERROROFF
RegWrite,HKLM,0x2,Tmp_System\ControlSet001\Services\SysmonLog,DefaultLogFileFolder,%temp%\PerfLogs
//RegWrite ! Overwrote existing key "NT Authority\NetworkService" with different data
If,%NoWarnROW%,Equal,True,System,ERROROFF
RegWrite,HKLM,0x1,Tmp_System\ControlSet001\Services\SysmonLog,ObjectName,LocalSystem
RegWrite,HKLM,0x4,Tmp_System\ControlSet001\Enum\Root\LEGACY_SYSMONLOG,NextInstance,1
RegWrite,HKLM,0x1,Tmp_System\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000,Service,SysmonLog
RegWrite,HKLM,0x4,Tmp_System\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000,Legacy,1
RegWrite,HKLM,0x4,Tmp_System\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000,ConfigFlags,0
RegWrite,HKLM,0x1,Tmp_System\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000,Class,LegacyDriver
RegWrite,HKLM,0x1,Tmp_System\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000,ClassGUID,{8ECC055D-047F-11D1-A537-0000F8753ED1}
RegWrite,HKLM,0x1,Tmp_System\ControlSet001\Enum\Root\LEGACY_SYSMONLOG\0000,DeviceDesc,%TMP%
RegHiveUnLoad,Tmp_System

