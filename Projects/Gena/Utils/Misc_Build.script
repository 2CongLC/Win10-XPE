[Variables]
%FileContainer%=%ScriptDir%\Builder_File.script

[Process]
Echo,"BE smart"

[BuildCMD_Parameters_Info]
run=.....Ex: /run="%BaseDir%\Projects\Gena"
title=...Ex: /title Gena
x=.......Location on Desktop (from top)
y=.......Location on Desktop (from left)
target=..Target Folder
ISO=.....ISO File
log=.....Log File
source=..Source_Dir
showlog..Open Log at then end of build
debug....Open message box to display project, target, source and ISO ...
beep.....At the end of build give Beep sound

[P_Button_BuildProject]
If,%ProjectName%,Equal,Gena,Run,%ScriptFile%,BuildCMD,%ScriptFile%,BuildGena,_ExeName_
Else,Run,%ScriptFile%,BuildCMD,%ScriptFile%,BuilderSE,_ExeName_

[P_Button_BuildGena]
If,Not,ExistFile,%BaseDir%\Projects\Gena\script.project,Call,Echo,"ERROR: Gena not found - Halt",Warn,,MessageError,5,Halt
//Run,%ScriptFile%,BuildCMD,%ScriptFile%,BuildGena,_ExeName_,"/run=#$q%BaseDir%\Projects\Gena#$q log=#$q%BaseDir%\Log.html#$q /debug"
Run,%ScriptFile%,BuildCMD,%ScriptFile%,BuildGena,_ExeName_,"/run=#$q%BaseDir%\Projects\Gena#$q /debug"

[BuildCMD]
//Run,"%ProjectDir%\Misc_Build.script",BuildCMD,"%ProjectDir%\Misc_Build.script"
//--
// 1 ScriptFile
// 2 Build Build80 BuildGena         Default: Build
// 3 Variable to be used for ExeName Default: %_ExeName_%
// 4 Location of project folder -->  Default: %ProjectDir%
// 5 Use Custom Parameter
If,Not,#5-,Equal,-,Set,#4,Dummy
//If,#5-,Equal,-,If,#4,Equal,-,Set,#4,"/run=#$q%ProjectDir%#$q"
If,#5-,Equal,-,If,#4,Equal,-,Set,#4,"/run=#$q%ProjectDir%#$q  /debug"
//-
If,#5-,Equal,-,Set,#5,#4
If,#3-,Equal,-,Set,#3,_ExeName_
If,#2-,Equal,-,Set,#2,BuilderSE
If,#1-,Equal,-,Set,#1,%ScriptFile%
//----
Run,#1,P_Extract_Base,#1,#2,#3
//----
Echo,"Start Building - %#3%.exe '#5'"
//--
If,#2,Equal,BuildGena,Begin
System,FILEREDIRECT,OFF
ShellexecuteEx,Open,%BaseDir%\%#3%.exe,"#5",%BaseDir%
System,FILEREDIRECT,ON
End

[P_Extract_Base]
If,#1-,Equal,-,Set,#1,%ScriptFile%
If,#2-,Equal,-,Halt,"Parameter 2 Missing - Build Build80 BuildGena"
//3 Name of Final Variable for exe : %BaseDir%\%exe%.exe
If,#3-,Equal,-,Set,#3,_ExeName_
Set,#9,0
//First Delete ALL Previous Temp files
Loop,#1,P_Del_Temp_Loop,1,100,#9,#2,#1
//Extract Main File if not exists
If,Not,ExistFile,%BaseDir%\#2.exe,Run,#1,Extract_ProjectTemp,#1,#2,#3
//--
Loop,#1,P_Find_Temp_Loop,1,100,#9,#2,#3
//--
//As Default Add BaseDir
Run,#1,Extract_ProjectTemp_#2,#1
If,#2,Equal,BuildGena,Begin
If,Not,ExistFile,"%BaseDir%\BuildGena Starter.exe",Run,#1,Extract_ProjectTemp_BuildGena_Starter,#1,#2,#3
FileCopy,%ProjectTemp%\#2.exe,%BaseDir%
End
//--
Run,#1,Create_INI_File,%BaseDir%\%#3%.ini,#2
//--
FileCopy,%ProjectTemp%\#2.exe,%BaseDir%\%#3%.exe
If,#2,Equal,BuildGena,Begin
StrFormat,SPLIT,%#3%,_,3,#7
FileCopy,"%ProjectTemp%\BuildGena Starter.exe","%BaseDir%\BuildGena_Temp_#7 Starter.exe"
//Set,%#3%,"BuildGena_Temp_#7 Starter"
End

[P_Find_Temp_Loop]
StrFormat,Inc,#1,1
If,Not,ExistFile,%BaseDir%\#2_Temp_#1.exe,Set,%#3%,#2_Temp_#1
If,Not,ExistFile,%BaseDir%\#2_Temp_#1.exe,Loop,Break

[P_Del_Temp_Loop]
StrFormat,Inc,#1,1
If,ExistFile,%BaseDir%\#2_Temp_#1.exe,Run,#3,P_Del_Temp_Loop_Del,#1,#2

[P_Del_Temp_Loop_Del]
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%BaseDir%\#2_Temp_#1.exe#$q&del /q /f /A:R /A:H /A:S /A:A #$q%BaseDir%\#2_Temp_#1.exe#$q"
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%BaseDir%\#2_Temp_#1.exe#$q&del /q /f /A:R /A:H /A:S /A:A #$q%BaseDir%\#2_Temp_#1.exe#$q"
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%BaseDir%\#2_Temp_#1.exe#$q&del /q /f /A:R /A:H /A:S /A:A #$q%BaseDir%\#2_Temp_#1.exe#$q"
If,Not,ExistFile,%BaseDir%\#2_Temp_#1.exe,Begin
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%BaseDir%\#2_Temp_#1.ini#$q&del /q /f /A:R /A:H /A:S /A:A #$q%BaseDir%\#2_Temp_#1.ini#$q"
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%BaseDir%\#2_Temp_#1.ini#$q&del /q /f /A:R /A:H /A:S /A:A #$q%BaseDir%\#2_Temp_#1.ini#$q"
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%BaseDir%\#2_Temp_#1.ini#$q&del /q /f /A:R /A:H /A:S /A:A #$q%BaseDir%\#2_Temp_#1.ini#$q"
End
If,#2,Equal,BuildGena,Begin
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%BaseDir%\#2 Starter_Temp_#1.exe#$q&del /q /f /A:R /A:H /A:S /A:A #$q%BaseDir%\#2 Starter_Temp_#1.exe#$q"
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%BaseDir%\#2 Starter_Temp_#1.exe#$q&del /q /f /A:R /A:H /A:S /A:A #$q%BaseDir%\#2 Starter_Temp_#1.exe#$q"
ShellExecute,Hide,cmd.exe,"/C del /q /f #$q%BaseDir%\#2 Starter_Temp_#1.exe#$q&del /q /f /A:R /A:H /A:S /A:A #$q%BaseDir%\#2 Starter_Temp_#1.exe#$q"
End

[Create_INI_File]
